{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends":["airbnb-base", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-undef": "off",
    "no-path-concat": "off",
    "prefer-template": "off"
  }
}
