                "message": {
                  "type": "string",
                  "example": "成功登入!!!"
                },
                "token": {
                  "type": "string",
                  "example": "yourToken..."
                },
                "user": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "number",
                      "example": 11
                    },
                    "name": {
                      "type": "string",
                      "example": "Johnny1"
                    },
                    "email": {
                      "type": "string",
                      "example": "user1@example.com"
                    },
                    "role": {
                      "type": "string",
                      "example": "user"
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "所有欄位必填, 帳號必須存在, 否則回傳error物件",
            "schema": {
              "type": "object",
              "properties": {
