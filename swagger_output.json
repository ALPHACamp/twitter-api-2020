              "type": "object",
              "properties": {
                "account": {
                  "type": "string",
                  "example": "account"
                },
                "email": {
                  "type": "string",
                  "example": "example@example.com"
                },
                "password": {
                  "type": "string",
                  "example": "87654321"
                },
                "checkPassword": {
                  "type": "string",
                  "example": "87654321"
                },
                "name": {
                  "type": "string",
                  "example": "Jack"
