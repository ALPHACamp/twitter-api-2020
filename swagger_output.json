    "/api/users/{id}/account": {
      "put": {
        "tags": [
          "User"
        ],
        "description": "修改使用者帳號, email, 密碼, 名稱。帳號、信箱、名稱必填",
        "parameters": [
          {
            "name": "id",
            "in": "path",
